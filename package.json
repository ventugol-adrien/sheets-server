{
  "name": "sheets-server",
  "type": "module",
  "version": "1.0.0",
  "description": "returns google sheets API data",
  "license": "ISC",
  "author": "adrien.ventugol",
  "main": "index.js",
  "scripts": {
    "newId": "npx uuid v4",
    "test": "test",
    "compile": "tsc -b",
    "deploy": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "redeploy": "docker build --no-cache -t server . && docker tag server ventugoladrien/sheets-server:server && docker push ventugoladrien/sheets-server:server && gcloud run deploy sheets-server2 --image docker.io/ventugoladrien/interactive-resume:server",
    "start": "node ./dist/index.js",
    "dev": "set NODE_ENV=DEV&& set PORT=54337&& npm start",
    "restart": "tsc -b && npm run dev"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@google/generative-ai": "^0.22.0",
    "@types/gapi": "^0.0.47",
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "mongodb": "^6.13.1",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.1"
  }
}
